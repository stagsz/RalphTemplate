# find.bi Environment Configuration
# Copy to .env and fill in your values: cp .env.example .env

# === Database ===
DATABASE_URL=postgresql://ralph:ralph@localhost:5432/ralph

# === Security ===
# Generate with: python -c "import secrets; print(secrets.token_urlsafe(32))"
JWT_SECRET=change-me-to-a-random-secret

# === DuckDB ===
# Path where per-workspace DuckDB files are stored
DUCKDB_PATH=./data/workspaces

# === AI Services (optional â€” app works without these) ===
ANTHROPIC_API_KEY=
OPENAI_API_KEY=

# === Frontend ===
VITE_API_URL=http://localhost:8000
